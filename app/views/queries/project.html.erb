<FORM><INPUT Type="button" VALUE="Back" onClick="history.go(-1);return true;"></FORM>
<%= form_tag tests_project_path do %>
  <% if current_user %>
    <h4>Hello, <%= current_user.name %></h4>
    <h4>Search Projects by partners</h4>
    <%#= collection_select(:operating_company_name, :search, Operating.all, :id, :operating_company_name) %>
     <%= text_field_tag :search, nil, placeholder: "Insert Service Company", class: "form-control" %>
     <%= text_field_tag :search1, nil, placeholder: "Insert Operating Company", class: "form-control" %>
     <%= submit_tag "Submit", class: "btn btn-primary" %>
  <% end %>
<% end %>







<div class="page-header"><h3>Projects</h3></div>
  <div class="span3">
    <table align="" border="1px" class="table table-striped">
      
      <th>Operator Company Name</th>
      <th>Service Company Name</th>
      <th>Project Title</th>
      <th>Start Year</th>
      <th>End Year</th>
      
            
       
      <% @fields.each_row do |item| %>
         <tr><td><%= item[0]  %></td>
        	 <td><%= item[1]  %></td>
        	 <td><%= item[2]  %></td>
        	 <td><%= item[3]  %></td>
        	 <td><%= item[4]  %></td>
      <% end %>
    
    </table>
</div>