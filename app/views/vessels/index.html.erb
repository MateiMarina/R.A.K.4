<div class="container">
<nav>
<ul class="breadcrumb">
  <li><%= link_to "Home",  static_pages_data_tables_path %></li>
</ul>
</nav>
              
                      
<div class="page-header" align="center"><h3>Vessels Information </h3></div>
<div class="span3">
<table align="" border="1px"  cellspacing="5px" class="table table-striped">
 <center><%= will_paginate  @vessel %></center>
  
  <th>Vessel Name</th>
  <th>Latitude/Longitude </th>
   <th>Vessel Status</th>
     <th>Flag/Country</th>
      <th>Vessel Type</th>
      <th>IMO</th>
      
  <% @vessel.each do |row| %>
       <tr><td><%= row.vessel_name%></td>
  <td><%= row.latitude%></td>
  <td><%= row.vessel_status%></td>
  <td><%= row.country%></td>
  <td><%= row.vessel_type%></td>
  <td><%= row.imo_no%></td></tr>
  
<% end %>
 <center><%= will_paginate @vessel %></center>
 </table>
</div>
</div>



