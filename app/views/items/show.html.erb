<div class="container">
<nav>
<ul class="breadcrumb">
  <li><%= link_to "Home",   static_pages_data_tables_path %></li>
  <li><%= link_to "Add new asset", new_item_path  %></li>
   <li><%= link_to "Vessels Information",vessels_path  %></li>
</ul>
</nav>
              
                      
<div class="page-header"><h3>Assets</h3></div>
<div class="span3"> 
<table align="" border="1px"  cellspacing="5px" class="table table-striped">
   <th>Asset Name</th>
    <th>Asset type</th>
     <th>Asset owner</th>
       <th>Current manager</th>
        <th>Previous manager</th>
         <th>Asset specification</th>
          <th>Edit</th>
           <th>Delete</th>

 
   <tr><td><%= @asset.assets_name%></td>
       <td><%= @asset.asset_type%></td>
        <td><%= @asset.asset_owner%></td>
           <td><%= @asset.current_manager%></td>
            <td><%= @asset.previous_manager%></td>
             <td><%= @asset.asset_specifications%></td>

           <td><%= link_to "Edit", edit_item_path(@asset),data: { confirm: " Are you Sure you want to edit?" }   %></td>
          <td><%= link_to "Delete", item_path(@asset), method: :delete,
          data: { confirm: " Are You sure you want to Delete?" } %></td></tr> 
          

    </table>
    
</div>
</div>